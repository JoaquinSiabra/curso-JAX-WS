<?xml version="1.0" encoding="windows-1252"?>
<project name="GeneracionCliente" default="GenerarCliente" basedir=".">
  <property name="UbicacionWSDL" value="http://localhost:7001/EjemploServicioHoraFromJava/ImplementacionServicioHoraService?wsdl"/>
  <property name="fuentes.dir" location="${basedir}/src"/>
  <property name="compilados.dir" value="${basedir}/build"/>
  <property name="JAXBbinding.file" value="${basedir}/CrearClienteAsincrono.xml"/>
  <property name="paquetedestino" value="clienteserviciohoraasync"/>
  <!--<property name="wsImport" value="wsimport.exe"/>-->
  <property name="wsImport" value="C:\ProyectoFinCarrera\SwBase\Oracle\Middleware\jdk160_29\bin\wsimport.exe"/>
  <target name="GenerarCliente">
  	<exec executable="${wsImport}">
	  	<!-- WSDL -->
  		<arg value="${UbicacionWSDL}"/>
      	<arg value="-keep"/>
	    <!-- Proxy -->
	    <arg value="-httpproxy:galeon.central.sepg.minhac.age:8080"/>
	    <!-- Directorio para fuentes -->
	    <arg value="-s"/>
	    <arg value="${fuentes.dir}"/>
		<!-- Directorio para compilados -->
		<arg value="-d"/>
		<arg value="${compilados.dir}"/>
		<!-- Paquete destino -->
	    <arg value="-p"/>
		<arg value="${paquetedestino}"/>
	    <!-- Solicita no compilar las clases generadas -->
	    <arg value="-Xnocompile"/>
		<!-- Muestra mensajes durante la generación -->
		<arg value="-verbose"/>
		<arg value="-b"/>
		<arg value="${JAXBbinding.file}"/>
	</exec>
  </target>
</project>